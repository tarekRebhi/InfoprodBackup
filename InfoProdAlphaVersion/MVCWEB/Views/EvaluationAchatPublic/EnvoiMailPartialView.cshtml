@model MVCWEB.Models.EvaluationAchatPublicViewModel
<div class="modal fade" id="EnvoiMailResult" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content" style="background-image:url('../../Content/template/Layout 1/.JPG');">
            <div class="modal-header" style="background-color:white;">
                <button type="button" id="fermer" class="close" data-dismiss="modal">&times;</button>
                <h5><font color="black"> Evaluation ajoutée </font></h5>
            </div>
            <div class="modal-body">
                @*<p> Succés d'envoi du mail à <label>@Html.DisplayFor(modelItem => modelItem.agentName)</label></p>*@
                <p> Evaluation ajoutée avec succès à <label>@Html.DisplayFor(modelItem => modelItem.agentName)</label></p>
            </div>
        </div>
    </div>
</div>

