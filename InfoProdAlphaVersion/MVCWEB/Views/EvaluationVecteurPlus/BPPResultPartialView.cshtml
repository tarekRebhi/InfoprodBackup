@model MVCWEB.Models.EvaluationVecteurPlusViewModel
<div class="modal fade" id="BPPResult" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content" style="background-image:url('../../Content/template/Layout 1/.JPG');">
            <div class="modal-header" style="background-color:#26A69A;">
                <button type="button" id="fermer" class="close" data-dismiss="modal">&times;</button>
                <h5><font color="white">Resultat : Evaluation BPP Vecteur Plus</font></h5>
            </div>
            <div class="modal-body">

                <div class="row">
                    <div class="col-lg-6">
                        <h6><font color="black"> <b>Date et temps d'évaluation</b></font></h6>
                    </div>
                    <div class="col-lg-4">
                        <a><h6>@Html.DisplayFor(modelItem => modelItem.dateTempEvaluation)</h6></a>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Nom Agent:</b></font></h6>
                        <div class="col-lg-4">
                            <a><h6>@Html.DisplayFor(modelItem => modelItem.agentName)</h6></a>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Présentation et Identification</b></font></h6>

                        <div class="col-xs-4">
                            <a> <h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.presentationIdentification)</h6></a>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Presenter l'objet de l'appel</b></font></h6>
                        <div class="col-xs-4">
                            <a> <h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.objetAppel)</h6></a>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Validation et confirmation de la Nature et Type, Lieu d'execution , Destination</b> </font> </h6>
                        <div class="col-xs-4">
                            @if (Model.validationNature >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.validationNature)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Calendrier</b></font> </h6>
                        <div class="col-xs-4">
                            @if (Model.calendrier >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.calendrier)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Envergure</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.envergure >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.envergure)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Complétude et précision des questions par Typologie</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.complétudeQuestions >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.complétudeQuestions)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Confirmation et ajout de tout les intervenants (surtout le bon MOA)</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.confirmationIntervenants >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.confirmationIntervenants)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Enchainement logique des questions</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.enchainementQuestions >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.enchainementQuestions)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Traitement des objections</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.traitementObjections >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.traitementObjections)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Reformulation et/ou Recap</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.reformulation >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.reformulation)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Transcription des informations</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.transcriptionsInformations >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.transcriptionsInformations)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Ajout des codes appropriés</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.codesAppropriés >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.codesAppropriés)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Note Client / Historique</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.noteClient >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.noteClient)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Discours</b></font></h6>
                        <div class="col-xs-4">
                            <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.discours)</h6></a>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Empathie / Attitude</b></font></h6>
                        <div class="col-xs-4">
                            <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.attitude)</h6></a>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Prise de congé</b></font></h6>
                        <div class="col-xs-4">
                            <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.priseConge)</h6></a>

                        </div>
                    </div>
                </div>
                <br />
                @{

                    String str = Model.pourcentageNotes.ToString("N2");
                }

                <p>
                    <h5 style="text-align:center"><b>Score: <font color="red">@str %</font></b> </h5>
                </p>
                <div class="text-right">

                </div>

            </div>

        </div>
        <div class="modal-footer" style="background-color:#26A69A;">
            <button type="button" class="btn btn-link" data-dismiss="modal">Fermer</button>


        </div>
    </div>
</div>

