@model MVCWEB.Models.DirectoryViewModel
<div class="modal fade" id="errorFile2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h5>Erreur : Pas d'Enregistrements Existants</h5>
                </div>
                <div class="modal-body">
                    <p>
                        <h10>
                            Le dossier que vous avez selectionné ne contient pas un enregistrement
                            correspondant a votre recherche pour l'agent @Html.DisplayFor(modelItem => modelItem.pseudoName)
                        </h10>
                    </p>
                </div>
            </div>
        </div>

 </div>

