@model MVCWEB.Models.DirectoryViewModel
<div class="modal fade" id="errorFile" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h5>Erreur : Dossier Inexistant</h5>
            </div>
            <div class="modal-body">
                <p>
                    <h10>
                        Le dossier que vous avez selectionné ne contient pas des sous dossiers
                        contenant la date de saisie entré.
                    </h10>
                </p>
            </div>
        </div>
    </div>

</div>