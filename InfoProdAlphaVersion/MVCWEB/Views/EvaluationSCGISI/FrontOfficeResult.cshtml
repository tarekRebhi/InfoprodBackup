@model MVCWEB.Models.EvaluationSCGISIViewModel
<div class="modal fade" id="FOResult" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content" style="background-image:url('../../Content/template/Layout 1/.JPG');">
            <div class="modal-header" style="background-color:#26A69A;">
                <button type="button" id="fermer" class="close" data-dismiss="modal">&times;</button>
                <h5><font color="white">Resultat : Evaluation Front Office SC GISI</font></h5>
            </div>
            <div class="modal-body">

                <div class="row">
                    <div class="col-lg-6">
                        <h6><font color="black"> <b>Date et temps d'évaluation</b></font></h6>
                    </div>
                    <div class="col-lg-4">
                        <a><h6>@Html.DisplayFor(modelItem => modelItem.dateTempEvaluation)</h6></a>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Nom Agent:</b></font></h6>
                        <div class="col-lg-4">
                            <a><h6>@Html.DisplayFor(modelItem => modelItem.agentName)</h6></a>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Accueil</b></font></h6>

                        <div class="col-xs-4">
                            <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.accueil)</h6></a>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Analyse/compréhension de la demande</b></font></h6>
                        <div class="col-xs-4">
                           
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.analyseDemande)</h6></a>
                          
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Maitrise des outils de travail (loggia/hcub)</b> </font> </h6>
                        <div class="col-xs-4">
                           
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.maitriseOutils)</h6></a>
                          
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Connaissance du produit</b></font> </h6>
                        <div class="col-xs-4">
                            @if (Model.connaissanceProduit >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.connaissanceProduit)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Respect des process de travail</b></font></h6>
                        <div class="col-xs-4">
                          
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.respectProcess)</h6></a>
                          
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Pertinence de la réponse</b></font></h6>
                        <div class="col-xs-4">
                          
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.pertinenceReponse)</h6></a>
                          
                        </div>
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>FCR</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.FCR >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.FCR)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Autonomie</b></font></h6>
                        <div class="col-xs-4">
                        
                       <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.autonomie)</h6></a>
                          
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Approche Commerciale</b></font></h6>
                        <div class="col-xs-4">
                            @if (Model.approcheCommerciale >= 0)
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.approcheCommerciale)</h6></a>
                            }
                            else
                            {
                                <a><h6 style="width:35%" class="col-xs-1 control-label">---</h6></a>
                            }
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Discours/empathie/attitude</b></font></h6>
                        <div class="col-xs-4">
                       
                             <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.discours)</h6></a>
                       
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Prise de congé</b></font></h6>
                        <div class="col-xs-4">
                          
                        <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.priseConge)</h6></a>
                        
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Identification du client CRM</b></font></h6>
                        <div class="col-xs-4">

                            <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.identificationClient)</h6></a>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-1">
                        <h6 style="width:50%" class="col-xs-1 control-label"><font color="black"><b>Qualification des demandes sur CRM
</b></font></h6>
                        <div class="col-xs-4">

                            <a><h6 style="width:35%" class="col-xs-1 control-label">@Html.DisplayFor(modelItem => modelItem.qualificationDemande)</h6></a>

                        </div>
                    </div>
                </div>
                <br />
                @{

                    String str = Model.pourcentageNotes.ToString("N2");
                }

                <p>
                    <h5 style="text-align:center"><b>Score: <font color="red">@str %</font></b> </h5>
                </p>
                <div class="text-right">

                </div>

            </div>

        </div>
        <div class="modal-footer" style="background-color:#26A69A;">
            <button type="button" class="btn btn-link" data-dismiss="modal">Fermer</button>


        </div>
    </div>
</div>

